{
    "name": "rueduphp/skeleton",
    "description": "Rue du PHP skeleton",
    "keywords": ["php", "framework", "rueduphp"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "GÃ©rald Plusquellec",
        "email": "gplusquellec@rueduphp.com",
        "homepage": "http://www.rueduphp.com",
        "role": "Developer"
    }],
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/rueduphp/octo.git"
    ]}
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "classmap": [
            "app/tasks",
            "app/entities"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "chmod 0777 -R app/storage",
            "chmod 0777 -R app/models"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
