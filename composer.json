{
    "name": "rueduphp/skeleton",
    "description": "Rue du PHP skeleton",
    "keywords": ["php", "framework", "rueduphp"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "GÃ©rald Plusquellec",
        "email": "gplusquellec@rueduphp.com",
        "homepage": "http://www.rueduphp.com",
        "role": "Developer"
    }],
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "php": ">=7.0",
        "illuminate/console": "@stable",
        "rueduphp/octo": "^2",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "illuminate/redis": "@stable",
        "http-interop/http-middleware": "^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "classmap": [
            "app/tasks",
            "app/entities"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "chmod 0777 -R app/storage",
            "chmod 0777 -R app/models"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
